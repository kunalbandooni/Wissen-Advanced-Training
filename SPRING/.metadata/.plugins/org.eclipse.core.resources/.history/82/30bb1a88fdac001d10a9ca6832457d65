package JUnitTest.JUnitTest;

import static org.junit.Assert.assertEquals;
import org.junit.Test;

public class App 
{

	public String login(String UserId, String Password){
	        if(UserId.equals("Kunal") && Password.equals("Admin"))    
	               return "Valid User";
	         else
	               return "Invalid User";
	}

	@Test
	public void loginTest(){
	     App testobj = new App();
	     String result = testobj.login("Kunal", "Admin");

	     assertEquals("Valid User", result);

	}
}
