package com.example.Product;

public class Product {
	private int productId;
	private String name;
	private String description;
	private String manufacturer;
	private int rating;
	private double price;
	
	public Product() {}
	public Product(int pId, String name, String description, String manufacturer, int rating, double price) {
		this.productId = pId;
		this.name = name;
		this.description = description;
		this.manufacturer = manufacturer;
		this.rating = rating;
		this.price = price;
	}
}